{
  "name": "quickllm",
  "version": "3.0.0",
  "description": "access various llms through a desktop app",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "electron .",
    "dev": "NODE_ENV=development electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.andrewcincotta.quickLLM",
    "productName": "QuickLLM",
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "package.json"
    ],
    "mac": {
      "target": ["dmg", "zip"]
    },
    "win": {
      "target": ["nsis", "zip"]
    },
    "linux": {
      "target": ["AppImage", "deb"]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewcincotta/quickLLM.git"
  },
  "keywords": [
    "llm",
    "ai",
    "electron"
  ],
  "author": "Andrew Cincotta",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/andrewcincotta/quickLLM/issues"
  },
  "homepage": "https://github.com/andrewcincotta/quickLLM#readme",
  "devDependencies": {
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1"
  }
}
